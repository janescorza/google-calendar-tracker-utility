{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2017",
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "allowJs": true,
    "isolatedModules": true,
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "types": ["react-native", "jest"]
  },
  "exclude": ["node_modules"]
}
